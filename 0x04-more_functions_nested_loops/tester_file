#include "main.h"
#include <stdio.h>

/**
 * is_upper - prototype function.
 *
 * Checks for uppercase character.
 *
 * @c: The character that needs to be checked
 * Return: Always 0.
 */
int _isupper(int c)
{
	char ch;

	printf("%i before for loop", c);
	for (ch = 'A'; ch <= 'Z'; ch++)
	{
		printf("I am in for loop\n");
		if(ch == c)
		{
			return (1);
		}
	}
	printf("I am outside for loop\n");
	return (0);
}
